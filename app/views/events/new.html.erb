<div class="row">
  <div class="col-md-8">
    <%= render 'form' %>
  </div>
  <div class="col-md-4">
    <div class="well">
      <center><h4>Events</h4></center>
      <table class="table">
        <% @events.each do |event| %>
            <tr>
              <td><%= event.title %></td>
              <td><%= event.date %></td>
            </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>